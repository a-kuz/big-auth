{
	"compilerOptions": {
		"baseUrl": ".",
		"noEmit": true,
		"module": "esnext",
		"allowSyntheticDefaultImports": true,
		"target": "ESNext",
		"lib": ["ESNext"],
		"experimentalDecorators": true,
    "emitDecoratorMetadata": true,
		"strict": true,
		"paths": {
      "~/*": ["src/*"]
    },
		"moduleResolution": "node16",
		"types": ["@cloudflare/workers-types", "@types/service-worker-mock"]
	},
	"exclude": ["node_modules"]
}
