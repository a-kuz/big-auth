{
	"compilerOptions": {
		"baseUrl": ".",
		"skipLibCheck": true,
		"noEmit": true,
		"module": "CommonJS",
		"allowSyntheticDefaultImports": true,
		"target": "ESNext",
		"lib": [
			"ESNext"
		],
		"experimentalDecorators": true,
		"emitDecoratorMetadata": true,
		"strict": true,
		"paths": {
			"~/*": [
				"src/*"
			]
		},
		"moduleResolution": "Node",
		"types": [
			"@cloudflare/workers-types",
			"@types/service-worker-mock"
		]
	},
	"exclude": [
		"node_modules"
	]
}
